
import random, easygui

inpt = []
outpt = []
kolvo = int(easygui.enterbox("сколько чисел желаете ввести:"))
for q in range(0, kolvo):
    number = int(easygui.enterbox("Вводите по одному числу после каждого нажимая Enter:"))
    inpt.append(number)
print inpt

f = kolvo + 1
for t in range(1, f):
    number = inpt[0]
    f = f - 1
    for sled in range(1, f):
        sled = inpt[sled]
        if sled < number:
            number = sled
        else:
            number = number
        
    inpt.remove(number)
    outpt.append(number)
print "hgd", outpt
print inpt
